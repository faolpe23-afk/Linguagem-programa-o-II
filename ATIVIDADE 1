package Faculdade;
// Classe base
public class Pessoa {
    private String nome;
    private int idade;
    private String endereco;

    public String getNome() {
        return nome;
    }

    public void setNome(String nome) {
        this.nome = nome;
    }

    public int getIdade() {
        return idade;
    }

    public void setIdade(int idade) {
        this.idade = idade;
    }

    public String getEndereco() {
        return endereco;
    }

    public void setEndereco(String endereco) {
        this.endereco = endereco;
    }

    public void showData() {
        System.out.println(nome + " - " + idade + " - " + endereco);
    }
} 
package Faculdade;
// Classe derivada PessoaFisica
public class PessoaFisica extends Pessoa {
    private String CPF;

    public String getCPF() {
        return CPF;
    }

    public void setCPF(String CPF) {
        this.CPF = CPF;
    }

public void showData() {
        System.out.println(getNome() + " - " + getIdade() + " - " + getEndereco() + " - CPF: " + CPF);
    }
}
package Faculdade;

//Classe derivada PessoaJuridica
public class PessoaJuridica extends Pessoa {
 private String CNPJ;

 public String getCNPJ() {
     return CNPJ;
 }

 public void setCNPJ(String CNPJ) {
     this.CNPJ = CNPJ;
 } 



public void showData() {
     System.out.println(getNome() + " - " + getIdade() + " - " + getEndereco() + " - CNPJ: " + CNPJ);
 }
}

package Faculdade;

public class Main {
    public static void main(String[] args) {
        PessoaFisica pf = new PessoaFisica();
        pf.setNome("Luis");
        pf.setIdade(30);
        pf.setEndereco("Santana");
        pf.setCPF("");
        pf.showData();
 PessoaJuridica pj = new PessoaJuridica();
        pj.setNome("Empresa X");
        pj.setIdade(10);
        pj.setEndereco("Centro");
        pj.setCNPJ("12.345.678/0001-99");
        pj.showData();
    }
}
